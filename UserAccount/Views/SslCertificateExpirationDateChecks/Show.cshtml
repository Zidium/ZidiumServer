@model Zidium.UserAccount.Models.SslCertificateExpirationDateChecksModels.EditModel

@{
    ViewBag.Title = "Просмотр проверки SSL-сертификата";
}

<style>
    .smart-col-labels {
        min-width: 280px;
    }
</style>

<p class="submenu-title">@ViewBag.Title</p> 

<div class="zi-bread-crumbs">
    @Html.Partial("~/Views/Controls/UnitTestBreadCrumbs.cshtml", Model.UnitTest)
</div>

<hr class="submenu" />

<div class="smart-container">
    @Html.Partial("~/Views/Checks/ShowCheckCommonSettings.cshtml", Model)
    
    @Html.DisplayFor(m => m.Url, "SmartDisplay")
    @Html.DisplayFor(m => m.AlarmDaysCount, "SmartDisplay")
    @Html.DisplayFor(m => m.WarningDaysCount, "SmartDisplay")

</div>
