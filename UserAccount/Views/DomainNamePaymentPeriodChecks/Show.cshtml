@model Zidium.UserAccount.Models.DomainNamePaymentPeriodCheckModels.EditModel
@{
    ViewBag.Title = "Просмотр проверки срока оплаты доменного имени";
} 

<style>
    .smart-col-labels {
        min-width: 280px;
    }
</style>

@Html.Partial("_PageTitle")

<div class="zi-bread-crumbs">
    @Html.Partial("~/Views/Controls/UnitTestBreadCrumbs.cshtml", Model.UnitTestBreadCrumbs)
</div>

<hr class="submenu" />

<div class="smart-container">
    @Html.Partial("~/Views/Checks/ShowCheckCommonSettings.cshtml", Model)
    
    @Html.DisplayFor(m => m.Domain, "SmartDisplay")
    @Html.DisplayFor(m => m.AlarmDaysCount, "SmartDisplay")
    @Html.DisplayFor(m => m.WarningDaysCount, "SmartDisplay")

</div>
