@model Zidium.UserAccount.Models.SqlChecksModels.EditModel

@{
    ViewBag.Title = "Просмотр проверки Sql";
}  

<style>
    .smart-col-labels {
        min-width: 260px;
    }
</style>

<p class="submenu-title">@ViewBag.Title</p> 

<div class="zi-bread-crumbs">
    @Html.Partial("~/Views/Controls/UnitTestBreadCrumbs.cshtml", Model.UnitTestBreadCrumbs)
</div>

<hr class="submenu" />

<div class="smart-container">
    @Html.Partial("~/Views/Checks/ShowCheckCommonSettings.cshtml", Model)
    
    @Html.DisplayFor(m => m.OpenConnectionTimeoutMs, "SmartDisplay")
    @Html.DisplayFor(m => m.CommandTimeoutMs, "SmartDisplay")
    @Html.DisplayFor(m => m.Provider, "SmartDisplay")
    @Html.DisplayFor(m => m.ConnectionString, "SmartDisplay")
    @Html.DisplayFor(m => m.Query, "SmartDisplay")

</div>